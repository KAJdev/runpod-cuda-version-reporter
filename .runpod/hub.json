{
  "title": "Runpod Cuda Version Reporter",
  "description": "This is a simple tool to report the cuda version of the current environment",
  "type": "serverless",
  "category": "utility",
  "iconUrl": "https://avatars.githubusercontent.com/u/37021919",
  "presets": [
    {
      "name": "Preset 1",
      "config": {
        "runsOn": "GPU",
        "env": [
          {
            "key": "STATIC_1",
            "value": "value_1_new"
          },
          {
            "key": "STRING_1",
            "input": {
              "name": "String Input",
              "type": "string",
              "description": "A string input test",
              "default": "new default value"
            }
          },
          {
            "key": "OPTION_1",
            "input": {
              "name": "Option Input",
              "type": "string",
              "description": "An option input test",
              "options": [
                {
                  "label": "value 1",
                  "value": "value_1"
                },
                {
                  "label": "value 2",
                  "value": "value_2"
                },
                {
                  "label": "value 3",
                  "value": "value_3"
                }
              ],
              "default": "value_2"
            }
          },
          {
            "key": "NUMBER_1",
            "input": {
              "name": "Number Input",
              "type": "number",
              "description": "A number input test",
              "min": 5,
              "max": 20,
              "default": 15
            }
          },
          {
            "key": "BOOLEAN_1",
            "input": {
              "type": "boolean",
              "name": "Boolean Input",
              "description": "A boolean input with toggle values",
              "default": true,
              "trueValue": "this is true",
              "falseValue": "this is false"
            }
          }
        ]
      }
    },
    {
      "name": "Preset 2",
      "config": {
        "runsOn": "GPU",
        "env": [
          {
            "key": "STATIC_2",
            "value": "value_2"
          },
          {
            "key": "STRING_2",
            "input": {
              "name": "String Input",
              "type": "string",
              "description": "A string input test",
              "default": "default value"
            }
          },
          {
            "key": "OPTION_2",
            "input": {
              "name": "Option Input",
              "type": "string",
              "description": "An option input test",
              "options": [
                {
                  "label": "value 1",
                  "value": "value_1"
                },
                {
                  "label": "value 2",
                  "value": "value_2"
                },
                {
                  "label": "value 3",
                  "value": "value_3"
                }
              ],
              "default": "value_2"
            }
          },
          {
            "key": "NUMBER_2",
            "input": {
              "name": "Number Input",
              "type": "number",
              "description": "A number input test",
              "min": 5,
              "max": 20,
              "default": 15
            }
          },
          {
            "key": "BOOLEAN_2",
            "input": {
              "type": "boolean",
              "name": "Boolean Input",
              "description": "A boolean input with toggle values",
              "default": true,
              "trueValue": "this is true",
              "falseValue": "this is false"
            }
          }
        ]
      }
    },
    {
      "name": "Preset 3",
      "config": {
        "runsOn": "GPU",
        "env": [
          {
            "key": "STATIC_3",
            "value": "value_3"
          }
        ]
      }
    }
  ]
}
